{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, must-revalidate"
        }
      ]
    },
    {
      "source": "/*.webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, must-revalidate"
        }
      ]
    }
  ]
}
